{
  "builds": [
    {
      "src": "src/mcp_registry_search/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/mcp_registry_search/api.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key",
    "CRON_SECRET": "@cron_secret"
  },
  "crons": [
    {
      "path": "/api/cron/etl",
      "schedule": "0 0 * * *"
    }
  ]
}